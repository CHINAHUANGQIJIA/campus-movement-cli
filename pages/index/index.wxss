/**导航栏**/
.tab{
  height: 100rpx;
  width: 100%;
  overflow: hidden;
  line-height: 80rpx;
  font-size: 20px;
  white-space: nowrap;
  z-index: 99;
  background-color: white;
}


.tab-item {
  display: inline-block;
  width: 33%;
  font-weight: 600;
  font-size: 35rpx;
  text-align: center;
  height: 70rpx;

}

.tab-item.active {
  margin-top: 5rpx;
  color: rgb(255, 158, 32);
  font-weight: 600;
  position: relative;
  text-align: center;
}

.tab-item.active:after {
  text-align: center;
 
  content: "";
  display: block;
  height: 7rpx;
  width: 100%;
  background: rgb(255, 158, 32);
  position: absolute;
  bottom: 0;
  top: 70rpx;
  border-radius: 16rpx;
}
.item-ans {
  width: 100%;
  display: flex;
  flex-grow: row no-wrap;
  justify-content: space-between;
  padding: 30rpx;
  box-sizing: border-box;
  height: 180rpx;
  align-items: center;
  border-bottom: 1px solid #F2F2F2;
}

.avatar {
  width: 100rpx;
  height: 100rpx;
  position: relative;
  padding-right: 30rpx;
}

.avatar .img {
  width: 100%;
  height: 100%;
}

.avatar .doyen {
  width: 40rpx;
  height: 40rpx;
  position: absolute;
  bottom: -2px;
  right: 20rpx;
}

.expertInfo {
  font-size: 12px;
  flex-grow: 2;
  color: #B0B0B0;
  line-height: 1.5em;
}

.expertInfo .name {
  font-size: 16px;
  color: #000;
  margin-bottom: 6px;
}

.askBtn {
  width: 120rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  font-size: 14px;
  border-radius: 60rpx;
  border: 1px solid #4675F9;
  color: #4675F9;
}

.tab-content {
  margin-top: 100rpx;
}

.scoll-h {
  height: 100%;
}


